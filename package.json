{
	"name": "@valtlai/eslint-config",
	"version": "0.27.0",
	"description": "ESLint config for my personal projects",
	"author": "Valtteri Laitinen <dev@valtlai.fi>",
	"license": "MIT",
	"repository": "valtlai/eslint-config",
	"scripts": {
		"test": "eslint .",
		"prepack": "node .prepack",
		"install": "husky install && husky add pre-commit 'npm test'",
		"prepublishOnly": "pinst --disable",
		"postpublish": "pinst --enable"
	},
	"engines": {
		"node": "^10 || ^12 || >=14"
	},
	"devDependencies": {
		"eslint": "~7.14.0",
		"eslint-plugin-node": "~11.1.0",
		"eslint-plugin-unicorn": "~23.0.0",
		"husky": "^5.0.4",
		"pinst": "^2.1.1"
	},
	"peerDependencies": {
		"eslint": "~7.14.0",
		"eslint-plugin-node": "~11.1.0",
		"eslint-plugin-unicorn": "~23.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test"
		}
	},
	"eslintConfig": {
		"extends": [
			".",
			"./node"
		]
	},
	"keywords": [
		"eslint",
		"eslintconfig"
	]
}
