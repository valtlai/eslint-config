{
	"name": "@valtlai/eslint-config",
	"version": "0.19.1",
	"description": "ESLint config for my personal projects",
	"author": "Valtteri Laitinen <dev@valtlai.fi>",
	"license": "MIT",
	"repository": "valtlai/eslint-config",
	"files": [
		"**/*.js"
	],
	"scripts": {
		"test": "eslint .",
		"reset": "rm -rf node_modules package-lock.json && npm i"
	},
	"engines": {
		"node": "^10 || ^12 || >=14"
	},
	"devDependencies": {
		"eslint": "~7.9.0",
		"eslint-plugin-node": "~11.1.0",
		"eslint-plugin-unicorn": "~22.0.0",
		"pre-commit": "^1.2.2"
	},
	"peerDependencies": {
		"eslint": ">=7.9.0",
		"eslint-plugin-node": ">=11.1.0",
		"eslint-plugin-unicorn": ">=22.0.0"
	},
	"peerDependenciesMeta": {
		"eslint-plugin-node": {
			"optional": true
		},
		"eslint-plugin-unicorn": {
			"optional": true
		}
	},
	"pre-commit": [
		"test"
	],
	"eslintConfig": {
		"extends": [
			".",
			"./node"
		]
	},
	"keywords": [
		"eslint",
		"eslintconfig"
	]
}
