{
	"name": "@valtlai/eslint-config",
	"version": "0.24.0",
	"description": "ESLint config for my personal projects",
	"author": "Valtteri Laitinen <dev@valtlai.fi>",
	"license": "MIT",
	"repository": "valtlai/eslint-config",
	"files": [
		"**/*.js",
		"!prepack.js"
	],
	"scripts": {
		"test": "eslint .",
		"prepack": "node prepack",
		"reset": "rm -rf node_modules package-lock.json && npm i"
	},
	"engines": {
		"node": "^10 || ^12 || >=14"
	},
	"devDependencies": {
		"eslint": "~7.12.0",
		"eslint-plugin-node": "~11.1.0",
		"eslint-plugin-unicorn": "~23.0.0",
		"husky": "^4.3.0"
	},
	"peerDependencies": {
		"eslint": "~7.12.0",
		"eslint-plugin-node": "~11.1.0",
		"eslint-plugin-unicorn": "~23.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test"
		}
	},
	"eslintConfig": {
		"extends": [
			".",
			"./node"
		]
	},
	"keywords": [
		"eslint",
		"eslintconfig"
	]
}
